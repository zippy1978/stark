; ModuleID = 'main'
source_filename = "main"

%string = type { i8*, i64 }

@gg = constant [3 x i8] c"gg\00"

define internal i64 @main() {
entry:
  %a = alloca %string, align 8
  store %string { i8* getelementptr inbounds ([3 x i8], [3 x i8]* @gg, i32 0, i32 0), i64 2 }, %string* %a, align 8
  ret i64 2
}